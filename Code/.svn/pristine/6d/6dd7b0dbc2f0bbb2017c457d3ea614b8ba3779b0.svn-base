<div class="panel panel-default">
	<div class="panel-heading">Participations</div>
	<div class="panel-body">
			<button id="saveStates" type="button"
					class="btn pull-right btn-success">Sauver</button>
		<div class="dataTables_wrapper form-inline dt-bootstrap no-footer">
			<div class="row">
				<div class="col-sm-12">
					<table
						class="table table-striped table-bordered table-hover dataTable no-footer dtr-inline"
						aria-describedby="participationsTable_info"
						id="participationsTable" role="grid" style="width: 100%;">
						<thead>

							<tr role="row">
								<!-- For the .details -->
								<th></th>
								<!-- hidden id -->
								<th class="hidden"></th>
								<th class="sorting" tabindex="0"
									aria-controls="participationsTable" aria-sort="ascending"
									aria-label="Rendering engine : activate to sort column descending"
									style="width: 164px;">Nom Entreprise</th>
								<th class="sorting" tabindex="1"
									aria-controls="participationsTable" aria-sort="ascending"
									aria-label="Rendering engine : activate to sort column descending"
									style="width: 164px;">Rue</th>
								<th class="sorting" tabindex="2"
									aria-controls="participationsTable" aria-sort="ascending"
									aria-label="Rendering engine : activate to sort column descending"
									style="width: 164px;">Numero</th>
								<th class="sorting" tabindex="3"
									aria-controls="participationsTable" aria-sort="ascending"
									aria-label="Rendering engine : activate to sort column descending"
									style="width: 164px;">Commune</th>
								<th class="sorting" tabindex="4"
									aria-controls="participationsTable" aria-sort="ascending"
									aria-label="Rendering engine : activate to sort column descending"
									style="width: 164px;">Code Postal</th>
								<th class="sorting" tabindex="4"
									aria-controls="participationsTable" aria-sort="ascending"
									aria-label="Rendering engine : activate to sort column descending"
									style="width: 164px;">Etat</th>
							</tr>
						</thead>
						<tbody>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>