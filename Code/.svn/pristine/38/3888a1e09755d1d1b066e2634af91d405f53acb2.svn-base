<div class="panel panel-default">
	<div class="panel-heading">Zone Dashboard</div>
	<div class="panel-body">
		<div class="col-md-3">
			<table class="table table-striped table-bordered table-hover"
				id="eventsTable">
				<thead>
					<tr>
						<th>Journées Entreprises</th>
					</tr>
				</thead>
				<tbody>
				</tbody>
			</table>
		</div>
		<div class="col-md-7">
			<div class="panel panel-default">
