<div class="panel panel-default">
	<div class="panel-heading">Formulaire d'ajout d'une entreprise</div>
	<div class="panel-body">
		<!-- Start of the company creation form -->
		<form id="createCompanyForm">
			<div class="form-group">
				<label for="name">Nom de la société :</label> <input type="text"
					class="form-control" id="createCompanyFormName"
					name="Nom de la société">
			</div>
			<div class="form-group">
				<label for="dateFirstContact">Date de premier contact :</label> <input
					type="date" class="form-control"
					id="createCompanyFormDateFirstContact"
					name="Date de premier contact" />
			</div>
			<div class="form-group">
				<label for="street">Rue :</label> <input type="text"
					class="form-control" id="createCompanyFormStreet" name="Rue" />
			</div>
			<div class="form-group">
				<label for="number">Numero :</label> <input type="number"
					class="form-control" id="createCompanyFormNumber" name="Numero" />
			</div>
			<div class="form-group">
				<label for="box">Boite :</label> <input type="number"
					class="form-control" id="createCompanyFormBox" name="Boite" />
			</div>
			<div class="form-group">
				<label for="postalCode">Code postal :</label> <input type="number"
					class="form-control" id="createCompanyFormPostalCode"
					name="Code postal" />
			</div>
			<div class="form-group">
				<label for="municipality">Commune :</label> <input type="text"
					class="form-control" id="createCompanyFormMunicipality"
					name="Commune" />
			</div>
			<button class="btn btn-success" id="createCompanyFormSubmit">Créer</button>
		</form>
		<!-- End of create company form -->
	</div>
</div>
