<div class="panel panel-default">
	<div class="panel-heading">Personnes de contact</div>
	<!-- Panel Heading -->
	<div class="panel-body">
		<div class="dataTables_wrapper form-inline dt-bootstrap no-footer">
			<div class="row">
				<div class="col-sm-12">
					<table
