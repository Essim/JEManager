<div class="panel panel-default">
	<div class="panel-heading">Entreprises à inviter</div>
	<!-- Panel Heading -->
	<div class="panel-body">
		<div class="dataTables_wrapper form-inline dt-bootstrap no-footer">
			<div class="row">
				<div class="col-sm-12">
					<table
						class="table table-striped table-bordered table-hover dataTable no-footer dtr-inline"
						aria-describedby="companiesToInvite_info"
						id="companiesToInviteTable" role="grid" style="width: 100%;">
						<thead>
							<tr role="row">
								<th class="sorting" tabindex="0"
									aria-controls="companiesToInviteTable" aria-sort="ascending"
									aria-label="Rendering engine : activate to sort column descending"
									style="width: 164px;">Nom</th>
								<th class="sorting" tabindex="1"
									aria-controls="companiesToInviteTable" aria-sort="ascending"
									aria-label="Rendering engine : activate to sort column descending"
									style="width: 164px;">Rue</th>
								<th class="sorting" tabindex="2"
									aria-controls="companiesToInviteTable" aria-sort="ascending"
									aria-label="Rendering engine : activate to sort column descending"
									style="width: 164px;">Numéro</th>
								<th class="sorting" tabindex="3"
									aria-controls="companiesToInviteTable" aria-sort="ascending"
									aria-label="Rendering engine : activate to sort column descending"
									style="width: 164px;">Commune</th>
								<th class="sorting" tabindex="4"
									aria-controls="companiesToInviteTable" aria-sort="ascending"
									aria-label="Rendering engine : activate to sort column descending"
									style="width: 164px;">Code Postal</th>
								<th class="sorting" tabindex="5"
									aria-controls="companiesToInviteTable" aria-sort="ascending"
									aria-label="Rendering engine : activate to sort column descending"
									style="width: 164px;">Inviter</th>
								<th class="sorting" tabindex="6"
									aria-controls="companiesToInviteTable" aria-sort="ascending"
									aria-label="Rendering engine : activate to sort column descending">ID</th>
							</tr>
						</thead>
						<tbody>
						</tbody>
					</table>
				</div>
			</div>
		</div>
		<button class="btn btn-primary btn-lg btn-block login-button" id="inviteButton">Invite</button>
	</div>
</div>